{
  "version": 2,
  "routes": [
    {
      "src": "/simple",
      "dest": "api/simple.js"
    },
    {
      "src": "/health",
      "dest": "api/health.js"
    },
    {
      "src": "/graphql-simple",
      "dest": "api/graphql-simple.js"
    },
    {
      "src": "/graphql-fixed",
      "dest": "api/graphql-fixed.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    },
    "api/health.js": {
      "maxDuration": 10
    },
    "api/graphql-fixed.js": { "maxDuration": 30 },
    "api/graphql-simple.js": { "maxDuration": 30 }
  }
}